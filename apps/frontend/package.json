{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "check-types": "tsc --noEmit",
    "test": "yarn test:unit",
    "test:unit": "jest --config jest.config.mjs",
    "test:unit:coverage": "jest --config jest.config.mjs --coverage",
    "test:e2e": "playwright test",
    "test:all": "yarn test:unit && yarn test:e2e",
    "cy:open": "cross-env BABEL_ENV=coverage cypress open",
    "cy:run": "cross-env BABEL_ENV=coverage cypress run",
    "cy:run:headed": "cross-env BABEL_ENV=coverage cypress run --headed",
    "coverage:unit": "yarn test:unit:coverage",
    "coverage:e2e": "cross-env BABEL_ENV=coverage cypress run",
    "coverage:report": "node scripts/prepare-coverage.js && nyc report -t .nyc_output --reporter=lcov --reporter=text-summary --report-dir coverage/combined",
    "test:coverage": "yarn coverage:unit && yarn coverage:e2e && yarn coverage:report"
  },
  "dependencies": {
    "@apollo/client": "^3.12.11",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.1.7",
    "@mui/icons-material": "^6.1.7",
    "@playwright/test": "^1.55.1",
    "@reduxjs/toolkit": "^2.9.0",
    "@repo/ui": "*",
    "@tanstack/react-query": "^5.59.16",
    "axios": "^1.12.2",
    "graphql": "^16.9.0",
    "isomorphic-dompurify": "^2.16.0",
    "next": "^15.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "@datadog/browser-rum": "^5.26.0",
    "@datadog/browser-logs": "^5.26.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "eslint": "^9.34.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.49.1",
    "ts-jest": "^29.2.5",
    "typescript": "5.9.2",
    "@cypress/code-coverage": "^3.13.11",
    "babel-plugin-istanbul": "^6.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^13.15.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.2"
  }
}
